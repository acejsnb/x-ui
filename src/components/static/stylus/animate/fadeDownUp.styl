/*CSS动画*/
@keyframes fadeInDown
  from
    opacity 0
    transform translate3d(0, -100%, 0)
  to
    opacity 1
    transform translate3d(0, 0, 0)
@keyframes fadeOutUp
  from
    opacity 1
  to
    opacity 0
    transform translate3d(0, -100%, 0)

.fadeDownUp-enter-active
  animation fadeInDown .3s ease-in-out
.fadeDownUp-leave-active
  animation fadeOutUp .3s